{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "dist",
        "rootDir": "src",
        "strict": true,
        "resolveJsonModule": true,
        "noImplicitAny": true,
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules",
        "**/*.test.ts"
    ]
}




